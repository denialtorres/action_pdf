<style>
.pdfobject-container {
   width: 1000px;
   height: 700px;
}
</style>


<p style="color: green"><%= notice %></p>

<%# = render @post %>

<div id="pdf-container" data-pdf-base64="<%= @pdf %>"></div>

<div>
  <%= link_to "Download PDF", post_pdf_path(@post) %>
  <%= link_to "Edit this post", edit_post_path(@post) %> |
  <%= link_to "Back to posts", posts_path %>

  <%= button_to "Destroy this post", @post, method: :delete %>
</div>
